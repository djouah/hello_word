        "def solution(x, y):\n",
        "    def somme (k): \n",
        "      s_=0\n",
        "      for i in range(k): \n",
        "        s_+=i\n",
        "      return s_  \n",
        "    Y=1+somme(y)\n",
        "    s__=Y\n",
        "    step=1+y\n",
        "    for i in range(x-1): \n",
        "      s__+=step+i\n",
        "    return s__"
      ],
